{
  "flex-layout.row#text__one--home": {
    "children": [
      "flex-layout.col#text__two--home"
    ],
    "props": {
      "blockClass": "title-row",
      "fullWidth": true
    }
  },
  "flex-layout.col#text__two--home": {
    "children": [
      "rich-text#categories-title",
      "flex-layout.row#categories"
    ],
    "props": { 
      "width": "100%"
    }
  },

  "rich-text#categories-title": {
    "props": {
      "text": "Categor√≠as",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "font": "t-heading-1",
      "textColor": "c-on-emphasis",
      "blockClass": "categories-title"
    }
  },

  "flex-layout.row#categories": {
    "children": [
      "flex-layout.col#category-1",
      "flex-layout.col#category-2",
      "flex-layout.col#category-3",
      "flex-layout.col#category-4",
      "flex-layout.col#category-5",
      "flex-layout.col#category-6"
    ],
    "props": {
      "blockClass": "categories-row",
      "colSizing": "auto",
      "colJustify": "center",
      "colGap": 7,
      "preventHorizontalStretch": true
    }
  },

  "flex-layout.col#category-1": {
    "children": [
      "image#category-1",
      "rich-text#category-1"
    ],
